#!/bin/sh
# SCRIPTS_DIR/bin/jitsi

[ -h "${0}" ] &&
    script_path="$(/bin/readlink --canonicalize "${0}")" ||
    script_path="${0}"

. "${script_path%/*}/../lib/send_message.sh"

[ -d ~/Downloads/jitsi ] && {
    cd ~/Downloads/jitsi
    send_message "Jitsi" "Loading..."
    ant run
    send_message "Jitsi" "Finished"
} || {
    send_message -e "Jitsi" "Run Failed"
    exit 1
}
	
